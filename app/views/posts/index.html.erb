<div class="container">
  <div class="col-sm-offset-3 col-sm-6 col-xs-12">
    <div class="post-box">
      <% @posts.each do |post| %>
        <%= post.title %>
        <%= post.subtitle %>
        <%= image_tag post.picture.url(:medium) %>
        <%= post.description %>
      <% end %>
    </div>
  </div>
</div>